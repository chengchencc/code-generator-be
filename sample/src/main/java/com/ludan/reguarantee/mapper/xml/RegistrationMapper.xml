<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ludan.reguarantee.mapper.RegistrationMapper">
    <select id="findList" resultType="com.ludan.reguarantee.entity.Registration">
        select * from registration t
        where 1=1
           <if test="p.code != null">
               t.code = #{p.code}
           </if>
           <if test="p.custName != null">
               t.cust_name = #{p.custName}
           </if>
           <if test="p.custNature != null">
               t.cust_nature = #{p.custNature}
           </if>
           <if test="p.custCertType != null">
               t.cust_cert_type = #{p.custCertType}
           </if>
           <if test="p.custCertNo != null">
               t.cust_cert_no = #{p.custCertNo}
           </if>
    </select>
</mapper>
