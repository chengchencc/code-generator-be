<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ludan.demo.mapper.SingleMapper">
    <select id="findList" resultType="com.ludan.demo.entity.Single">
        select * from t_test_Single t
        where 1=1
           <if test="p.id != null">
               t.id = #{p.id}
           </if>
           <if test="p.propInt != null">
               t.propInt = #{p.propInt}
           </if>
           <if test="p.propString != null">
               t.propString = #{p.propString}
           </if>
           <if test="p.propBool != null">
               t.propBool = #{p.propBool}
           </if>
    </select>
</mapper>
