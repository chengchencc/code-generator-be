<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ludan.demo.mapper.SampleMapper">
    <select id="findList" resultType="com.ludan.demo.entity.Sample">
        select * from sample t
        where 1=1
           <if test="p.inputText != null">
               t.input_text = #{p.inputText}
           </if>
           <if test="p.number != null">
               t.number = #{p.number}
           </if>
    </select>
</mapper>
